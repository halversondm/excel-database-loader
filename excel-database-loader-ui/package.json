{
  "name": "excel-database-loader-ui",
  "version": "1.0.0",
  "description": "Display homweowners from a an Excel database in a React application with Tailwind CSS.",
  "private": true,
  "author": "Dan Halverson <daniel.m.halverson@gmail.com",
  "license": "MIT",
  "keywords": [
    "react",
    "javascript",
    "frontend",
    "web-app",
    "tailwind",
    "responsive"
  ],
  "homepage": ".",
  "main": "src/index.js",
  "scripts": {
    "start": "vite",
    "prebuild": "npm run test -- run",
    "build": "vite build",
    "test": "vitest",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "preview": "npm run build && serve -s build",
    "analyze": "npm run build && npx bundle-analyzer build/static/js/*.js"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "lucide-react": "^0.539.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^27.5.2",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.27",
    "prettier": "^3.0.0",
    "serve": "^14.2.0",
    "tailwindcss": "^3.3.3",
    "vite": "^7.1.2",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^3.2.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier"
    ],
    "plugins": [
      "react-hooks"
    ],
    "rules": {
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn",
      "no-unused-vars": "warn",
      "no-console": "warn"
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/react-app-skeleton.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/react-app-skeleton/issues"
  },
  "proxy": "http://localhost:8082"
}
